<!-- music_area  -->
<div class="music_area">
    <div class="container">
        <div class="row align-items-center justify-content-center">
            <div class="col-xl-10">
                <div class="row align-items-center">
                    <div class="col-xl-9 col-md-9">
                        <div class="music_field">
                            <div class="thumb">
                                <img src="album/<%= featuredTrack.album.image %>" alt="" class="album-art">
                            </div>
                            <div class="audio_name">
                                <div class="name">
                                    <h4><%= featuredTrack.title %></h4>
                                    <p><%= featuredTrack.created_at.getDate()+" "+featuredTrack.created_at.toLocaleString('default', { month: 'long' })+" "+featuredTrack.created_at.getFullYear() %></p>
                                </div>
                                <audio preload="auto" controls>
                                    <source src="track/<%= featuredTrack.track %>">
                                </audio>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- music_area end  -->

<!-- about_area  -->
<div class="about_area">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-xl-5 col-md-6">
                <div class="about_thumb">
                    <img class="img-fluid" src="artist/<%= featuredArtist.image %> " alt="">
                </div>
            </div>
            <div class="col-xl-7 col-md-6">
                <div class="about_info">
                    <h3><%= featuredArtist.name %> </h3>
                    <p>
                        <%= featuredArtist.about %> 
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
<!--/ about_area  -->

<!-- youtube_video_area  -->
<div class="youtube_video_area">
    <div class="container-fluid p-0">
        <div class="row no-gutters">
            <% latestAlbums.forEach(latestAlbum => { %>
                <div class="col-xl-3 col-lg-3 col-md-6">
                    <div class="single_video">
                        <div class="thumb">
                            <img src="album/<%= latestAlbum.image %> " alt="">
                        </div>
                        <div class="hover_elements">
                            <div class="video">
                                <a class="popup-video" href="https://www.youtube.com/watch?v=tsBy6je8KjM">
                                    <i class="fa fa-play"></i>
                                </a>
                            </div>
    
                            <div class="hover_inner">
                                <span><%= latestAlbum.created_at.getDate()+" "+latestAlbum.created_at.toLocaleString('default', { month: 'long' })+", "+latestAlbum.created_at.getFullYear() %></span>
                                <h3><a href="#"><%= latestAlbum.title %> </a></h3>
                            </div>
                        </div>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</div>
<!-- / youtube_video_area  -->

<!-- music_area  -->
<div class="music_area music_gallery">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <div class="section_title text-center mb-65">
                    <h3>Latest Tracks</h3>
                </div>
            </div>
        </div>
        <% latestTracks.forEach(latestTrack => { %>
            <div class="row align-items-center justify-content-center mb-20">
                <div class="col-xl-10">
                    <div class="row align-items-center">
                        <div class="col-xl-9 col-md-9">
                            <div class="music_field">
                                <div class="thumb">
                                    <img src="album/<%= latestTrack.album.image %> " alt="" class="album-art">
                                </div>
                                <div class="audio_name">
                                    <div class="name">
                                        <h4><%= latestTrack.title %></h4>
                                        <p><%= featuredTrack.created_at.getDate()+" "+featuredTrack.created_at.toLocaleString('default', { month: 'long' })+" "+featuredTrack.created_at.getFullYear() %></p>
                                    </div>
                                    <audio preload="auto" controls>
                                        <source src="track/<%= latestTrack.track %>">
                                    </audio>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        <% }) %>
    </div>
</div>
<!-- music_area end  -->

